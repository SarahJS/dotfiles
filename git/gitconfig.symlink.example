[hub]
    protocol = git
[alias]
    st = status -sb
    lg = log --graph --decorate --oneline
    lga = log --graph --decorate --oneline --all
    co = checkout
    promote = !~/.dotfiles/bin/git-promote
    wtf     = !~/.dotfiles/bin/git-wtf
    rank-contributors = !$ZSH/bin/git-rank-contributors
    count   = shortlog -sn
    clear = !(cd ./$(git rev-parse --show-cdup) && rm -r $(git status --porcelain | grep '^??' | sed s/^...//))
    checkout-all = !(cd ./$(git rev-parse --show-cdup) && git checkout -- $(git status --porcelain | grep '^ [MDA]' | sed s/^...//))
    git = !echo git $@ && git $!
    abort = !~/.dotfiles/bin/git-abort
    diff-here = !~/.dotfiles/bin/git-diff-here
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[core]
    excludesfile = ~/.gitignore
    editor = vim
[help]
    autocorrect = 1
[merge]
    tool = vimdiff
[diff]
    tool = vimdiff
[push]
	default = current
[mergetool]
	keepBackup = false
[diff "spaceman-diff"]
    command = ~/.gitignore/bin/spaceman-diff
[url "git@github.com:"]
    insteadOf = "gh:"
    insteadOf = "git://github.com"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
    pushInsteadOf = "https://github.com/"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
    pushInsteadOf = "https://gist.github.com/"
[user]
    name=AUTHORNAME
    email=AUTHOREMAIL
